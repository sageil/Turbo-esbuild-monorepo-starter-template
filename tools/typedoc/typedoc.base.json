  "entryPoints": ["./src/index.ts"],
  "exclude": ["**/node_modules/**"],
  "cleanOutputDir": true,
  "externalPattern": ["**/node_modules/**"],
  "excludeExternals": true,
  "compilerOptions": {
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
