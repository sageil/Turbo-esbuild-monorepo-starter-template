{
  "name": "@apps/example",
  "version": "1.0.0",
  "description": "Sample browser app",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "node build.js",
    "prod": "pnpm --filter=@apps/example --prod deploy ../../production-deployments/brwoser-prod/",
